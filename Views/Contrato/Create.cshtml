@model inmobiliaria_mvc.Models.Contrato

@{
    ViewData["Title"] = "Crear Contrato";
}

<h1>@ViewData["Title"]</h1>

<hr />
<div class="row">
    <div class="col-md-6">
        <form asp-action="Create" method="post">
            <partial name="_FormContrato" model="Model" />
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-secondary mt-3">Volver a Lista</a>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}