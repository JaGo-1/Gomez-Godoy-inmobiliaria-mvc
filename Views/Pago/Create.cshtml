@model inmobiliaria_mvc.Models.Pago

@{
    ViewData["Title"] = "Registrar Pago";
}

<h1>@ViewData["Title"]</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <partial name="_FormPago" model="Model" />
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-secondary mt-3">Volver a Lista</a>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}